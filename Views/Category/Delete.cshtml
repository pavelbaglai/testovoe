
@model WebApplication39.Models.Category
@{
    ViewBag.Title = "Удаление ";
}
<h1>Удалить</h1>

<div>
    <dl class="dl-horizontal">
        <dt>Имя</dt>
        <dd>
            @Html.DisplayFor(model => model.Name)
        </dd>
    </dl>

    <div>
        <form asp-controller="Category" asp-action="Delete" method="post">
            <div class="form-group">
                <input type="submit" class="btn btn-default" value="Удалить" />
            </div>
        </form>
    </div>
</div>
